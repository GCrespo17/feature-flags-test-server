{
  "name": "feature-flags-test-server",
  "version": "1.0.0",
  "description": "Servidor de prueba para las feature flags",
  "main": "jest.config.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "nest start --config nest-cli.json --exec \"node dist/database/seed-command.js\"",
    "seed:dev": "ts-node -r tsconfig-paths/register src/database/seed-command.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GCrespo17/feature-flags-test-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GCrespo17/feature-flags-test-server/issues"
  },
  "homepage": "https://github.com/GCrespo17/feature-flags-test-server#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^22.16.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "xlsx": "^0.18.5",
    "nest-commander": "^3.12.0",
    "feature-flags-nestjs-lib": "https://github.com/GCrespo17/feature-flags-nestjs-lib.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  }
